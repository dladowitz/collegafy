  %section#contact.section.section-center.section-contact
    .container
      %h2.section-title
        %span Contact Us
      %p Want to say hello? Want to know more about us? Give us a call or drop us an email and we will get back to you as soon as we can.
      .main-action
        %form{:role => "form"}
          .row
            .col-sm-6
              .form-group
                %label.sr-only Subject
                %input.form-control{:placeholder => "Subject", :type => "text"}/
              .form-group
                %label.sr-only Name
                %input.form-control{:placeholder => "Name", :type => "text"}/
              .form-group
                %label.sr-only Email
                %input.form-control{:placeholder => "Email", :type => "email"}/
            .col-sm-6
              .form-group
                %label.sr-only Message
                %textarea.form-control{:placeholder => "Message", :rows => "6", :style => "height: 100px"}
          %button.btn.btn-default{:type => "submit"} Send message

