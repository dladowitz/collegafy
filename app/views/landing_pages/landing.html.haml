  %section#testimonials.section.section-center.section-hilite.section-testimonial
    .container
      %h2.section-title
        %span What our clients say?
      %i.fa.fa-quote-left.fa-3x
      #carousel-example-generic.carousel.slide{"data-ride" => "carousel"}
        .carousel-inner
          .item.active
            %blockquote
              %p I love the new types of pens and erasers I get to try out
              %small
                Mark Mulholland
                %cite{:title => "Source Title"}
          .item
            %blockquote
              %p So happy to never have to think about whiteboard supplies again
              %small
                Jack Burly
                %cite{:title => "Source Title"} - Captivate Real Estate
          .item
            %blockquote
              %p You've made whiteboards fun again
              %small
                Some guy
                %cite{:title => "Source Title"} working at a startup
        %ol.carousel-indicators
          %li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
            %img.img-circle{:alt => "", :height => "64", :src => "assets/landing_page/mark.jpg", :width => "64"}/
          %li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
            %img.img-circle{:alt => "", :height => "64", :src => "assets/landing_page/jack.jpg", :width => "64"}/
          %li{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
            %img.img-circle{:alt => "", :height => "64", :src => "assets/landing_page/david.jpg", :width => "64"}/
  %section#pricing.section.section-center.section-pricing
    .container
      %h2.section-title
        %span Pricing Table Made Easy
      .pricing-table
        .row
          .col-md-3.col-sm-6
            .panel.panel-default
              %header.panel-heading
                %h1.pricing-header Two Guys In A Garage
                .the-price
                  $5
                  %span.subscript / month
              .panel-body
                %table.table
                  %tbody
                    %tr
                      %td 2 Markers
                    %tr.active
                      %td 1 Eraser
                    %tr
                      %td 1 Accessory

              %footer.panel-footer
                %a.btn.btn-default{:href => "#"} Sign up now
          .col-md-3.col-sm-6
            .panel.panel-default
              %header.panel-heading
                %h1.pricing-header Seed Money
                .the-price
                  $10
                  %span.subscript / month
              .panel-body
                %table.table
                  %tbody
                    %tr
                      %td 4 Markers
                    %tr.active
                      %td 2 Erasers
                    %tr
                      %td 2 Accessories

              %footer.panel-footer
                %a.btn.btn-default{:href => "#"} Sign up now
          .col-md-3.col-sm-6
            .panel.panel-primary
              %header.panel-heading
                %h1.pricing-header Venture Capital
                .the-price
                  $20
                  %span.subscript / month
              .panel-body
                %table.table
                  %tbody
                    %tr
                      %td 8 Markers
                    %tr.active
                      %td 4 Erasers
                    %tr
                      %td 4 Accessories
                    %tr.active
                      %td 1 Suprise

              %footer.panel-footer
                %a.btn.btn-primary{:href => "#"} Sign up now
          .col-md-3.col-sm-6
            .panel.panel-default
              %header.panel-heading
                %h1.pricing-header IPO
                .the-price
                  $50
                  %span.subscript / month
              .panel-body
                %table.table
                  %tbody
                    %tr
                      %td 20 Markers
                    %tr.active
                      %td 10 Erasers
                    %tr
                      %td 10 Accessories
                    %tr.active
                      %td 3 Suprises
              %footer.panel-footer
                %a.btn.btn-default{:href => "#"} Sign up now
